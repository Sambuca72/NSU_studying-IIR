SELECT DISTINCT MANUFACTURER.COMPANY
FROM MANUFACTURER, PRODUCT,CATEGORY
WHERE MANUFACTURER.RECIPE_ID=PRODUCT.RECIPE_ID AND PRODUCT.WARE = CATEGORY.WARE
AND CATEGORY.CLASS = "Fuel"
INTERSECT
SELECT DISTINCT MANUFACTURER.COMPANY
FROM MANUFACTURER, PRODUCT,CATEGORY
WHERE MANUFACTURER.RECIPE_ID=PRODUCT.RECIPE_ID AND PRODUCT.WARE = CATEGORY.WARE
AND CATEGORY.CLASS = "Food"
